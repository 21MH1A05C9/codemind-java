import java.util.*;
class print
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int s,i,sum=0;
        s=sc.nextInt();
        int arr[] = new int[s];
        for (i=0;i<s;i++)
        {
            arr[i]=sc.nextInt();
        }
        for (i=0;i<s;i++)
        {
            sum+=arr[i];
        }
        System.out.print(sum);
    }
}
