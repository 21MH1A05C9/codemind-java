import java.util.*;
class print
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n,i;
        float sum=0;
        n=sc.nextInt();
        for(i=1;i<=n;i++)
        {
            sum+=(1.0/i);
        }
        System.out.format("%.2f",sum);
    }
}
